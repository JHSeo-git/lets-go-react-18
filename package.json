  "source": "./src/index.tsx",
  "main": "./dist/index.js",
  "scripts": {
    "csr:clean": "rimraf dis",
    "ssr:clean": "rimraf dist-ssr",
    "dev": "cross-env NODE_ENV=development MODE=csr:dev webpack serve --open",
    "build": "yarn csr:clean && cross-env NODE_ENV=production MODE=csr:prod webpack --progress=profile",
    "dev:ssr": "nodemon",
    "build:ssr": "yarn ssr:clean && cross-env NODE_ENV=production yarn build:client && cross-env NODE_ENV=production yarn build:server",
    "build:client": "cross-env MODE=ssr:client webpack --progress=profile",
    "build:server": "cross-env MODE=ssr:server webpack --progress=profile",
    "lint": "eslint src",
    "format": "prettier --write 'src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@faker-js/faker": "^6.1.1",
    "@loadable/component": "^5.15.2",
    "@loadable/server": "^5.15.2",
