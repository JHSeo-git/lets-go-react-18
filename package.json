    "@types/randomcolor": "^0.5.6",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/webpack-env": "^1.16.3",
    "@types/webpack-hot-middleware": "^2.25.6",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-loader": "^8.2.4",
    "concurrently": "^7.0.0",
